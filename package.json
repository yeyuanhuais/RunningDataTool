{
  "name": "running-data-tool",
  "version": "1.0.0",
  "description": "Electron tool to visualize CSV metrics as line charts",
  "main": "main.js",
  "author": "",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "build:win": "electron-builder --win portable"
